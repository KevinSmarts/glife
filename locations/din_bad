$din_Switch_Condom_Menu = {
	dynamic $din_Update_Condom_Counts
	clr
	i = 0
	pl '妳有 <<iif(preziktype < 2, prezik+prezikcount, prezikcount)>> 枚正常, <<iif(preziktype = 2, prezik+preziksabo, preziksabo)>> 枚壞的保險套.'
	nl
	if preziktype > 0:
		PL '<a href="exec:dynamic $din_Do_Condom_Counts, 1">改用正常的保險套</a>.'
	end
	if preziktype ! 1:
		PL '<a href="exec:dynamic $din_Do_Condom_Counts, 2">停止使用保險套</a>.'
	end
	if preziktype ! 2:
		PL '<a href="exec:dynamic $din_Do_Condom_Counts, 3">改用壞的保險套</a>.'
	end
	if prezikcount > 0 or preziktype = 0:
		pl '<a href="exec:dynamic $din_Do_Condom_Counts, 4">戳壞一個保險套</a>.'
	end
	NL
	PL '<a href="exec:gs ''stat''">返回</a>'
}

$din_Table_Condom_Menu = {
	dynamic $din_Update_Condom_Counts
	*clr
	i = 1
	'You have <<iif(preziktype < 2, prezik+prezikcount, prezikcount)>> normal, <<iif(preziktype = 2, prezik+preziksabo, preziksabo)>> sabotaged condoms.'
	nl
	if preziktype > 0:
		'<a href="exec:dynamic $din_Do_Condom_Counts, 1">改用正常的保險套</a>.'
	end
	if preziktype ! 1:
		'<a href="exec:dynamic $din_Do_Condom_Counts, 2">停止使用保險套</a>.'
	end
	if preziktype ! 2:
		'<a href="exec:dynamic $din_Do_Condom_Counts, 3">改用壞的保險套</a>.'
	end
	if prezikcount > 0 or preziktype = 0:
		'<a href="exec:dynamic $din_Do_Condom_Counts, 4">戳壞一個保險套</a>.'
	end
	NL
	'<a href="exec:gt ''stol'', ''bc''">返回</a>'
}

$din_Do_Condom_Counts = {
	!{set condom count and type}
	if ARGS[0] = 1:
		clr
		'需要時, 妳會主動拿出正常的保險套.'
		if preziktype = 2: preziksabo = prezik
		prezik = prezikcount
		prezikcount = 0
		preziktype = 0
	elseif ARGS[0] = 2:
		'下次妳不會主動拿出保險套.'
		if preziktype = 0: prezikcount = prezik
		if preziktype = 2: preziksabo = prezik
		prezik = 0
		preziktype = 1
	elseif ARGS[0] = 3:
		'需要時, 妳會主動拿出壞的保險套.'
		if preziktype = 0: prezikcount = prezik
		prezik = preziksabo
		preziksabo = 0
		preziktype = 2
	elseif ARGS[0] = 4:
		if preziktype ! 0 and prezikcount > 0:
			preziksabo +=1
			prezikcount -=1
			'妳用針在保險套上戳了個洞, 並作好記號.'
		elseif preziktype = 0 and prezik > 0:
			preziksabo +=1
			prezik -=1
			'妳用針在保險套上戳了個洞, 並作好記號.'
		else
			'妳沒有正常的保險套了.'
		end
	end
	if i = 0:
		dynamic $din_Switch_Condom_Menu
	else
		dynamic $din_Table_Condom_Menu
	end
}
   
$din_Update_Condom_Counts = {
	if prezik + prezikcount + preziksabo > 0: prezikmsg = 1
	if preziktype = 0:
		if prezikcount > 0:
			prezik += prezikcount
			prezikcount = 0
		end
		if prezik <= 0:
			'保險套用完了.'
			prezik = 0
			prezikcount = 0
			preziktype = 1
		end
	elseif preziktype = 1:
		if prezik > 0:
			prezikcount += prezik
			prezik = 0
		end
	elseif preziktype = 2:
		if preziksabo > 0:
			prezik += preziksabo
			preziksabo = 0
		end
		if prezik <= 0 and prezikcount <= 0:
			if prezikmsg = 1:prezikmsg = 2
			prezik = 0
			preziksabo = 0
			prezikcount = 0
			preziktype = 1
		elseif prezik <= 0 and prezikcount > 0:
			'壞的都沒了, 所以妳改拿出正常的保險套.'
			prezik = prezikcount
			prezikcount = 0
			preziktype = 0
		end
	end
}

