$d_tabletkiedA = {
	if ARGS[0] = 1:
		tabletkipd += tabletkipt
		tabletki -= 1
		if tabletkiday ! daystart:
			pillcon += 1000+(pillcon+15000)/40
			if pillcon > 40000: pillcon = 40000
			tabletkiday = daystart
			tabletkipt = 0
			tabletkipi = 0
		else
			health -= 1
		end
		'妳吃了一錠避孕藥.'
		gs 'stat'
	elseif ARGS[0] = 2:
		'妳每天會記得吞一錠避孕藥.'
		tabletkicheck = 1
		gs 'stat'
	elseif ARGS[0] = 3:
		'妳自己決定何時要吃避孕藥.'
		tabletkicheck = 0
		gs 'stat'
	elseif ARGS[0] = 4:
		if tabletkioddp <= 0:
			'這些是普通的事前避孕藥, 每天一錠.'
			tabletkioddk = 0
			gs 'stat'
		else
			if Tabletki <= 30:
				'已經開封的那排藥看起來怪怪的, 也許是假的?'
			else
				'藥看起來怪怪的, 也許是假的?'
			end
			tabletkioddk = 1
			gs 'stat'	
			if tabloc = 0:
				dynamic $d_tabletkied
			else
				dynamic $d_tabletkiedt
			end
		end
	elseif ARGS[0] = 5:
		'妳把假的避孕藥丟掉.'
		tabletkioddk = 0
		tabletki -= 30
		tabletkioddp = 0
		if tabletki < 0:
			tabletki = 0
		end
		gs 'stat'
	end
	
	if tabloc = 0:
		dynamic $d_tabletkied
	else
		dynamic $d_tabletkiedt
	end
}

$d_cycreport_choice = {
	clr
	dynamic $d_cycreport_update
	PL '目前狀態: <<$cycreport_txt>>'
	if $cycreport_txt ! 'In labour':
		NL
		PL '改變週期計算, 因為:'
		if thinkpreg = 0:
			if knowpreg = 1:
				PL '<a href="exec: dynamic $d_cycreport_actor, 2">我懷孕了.</a>'
			else 
				PL '<a href="exec: dynamic $d_cycreport_actor, 2">我覺得我懷孕了.</a>'
			end
		elseif thinkpreg = 1:
			PL '<a href="exec: dynamic $d_cycreport_actor, 1">我沒有懷孕.</a>'
		end
		if mesec > 0 and thinkpreg ! 1 and knowpreg ! 1:
			PL '<a href="exec: dynamic $d_cycreport_actor, 3">我正在月經.</a>'
		elseif thinkpreg ! 1:
			PL '<a href="exec: dynamic $d_cycreport_actor, 4">我的月經剛結束.</a>'
			PL '<a href="exec: dynamic $d_cycreport_actor, 5">我覺得我的月經停了.</a>'
		end
		if thinkpreg = 1 or knowpreg = 1:
			PL '<a href="exec: dynamic $d_cycreport_actor, 6">我覺得我受精的日期是...</a>'
			PL '<a href="exec: dynamic $d_cycreport_actor, 7">我覺得孩子的父親是...</a>'
		end
	elseif cycle = 5:
		pl '羊水已經破了, 宮縮也已經開始, 很明顯妳要生了. -- 快, 妳得馬上到大醫院去!'
	else
		dynamic $d_cycreport_actor, 10
	end
	PL '<a href="exec:gs ''stat''">返回</a>'
}

$d_cycreport_actor = {
	if ARGS[0] = 1:
		if knowpreg = 1:
			denypreg = 1
			thinkpreg = 0
		else
			denypreg = 0
			thinkpreg = 0
			temp[1] = daystart-daylastperiod
		end
		dynamic $d_cycreport_update
		gs 'stat'
	elseif ARGS[0] = 2:
		thinkpreg = 1
		denypreg = 0
		dynamic $d_cycreport_update
		gs 'stat'
	elseif ARGS[0] = 3:
		thinkpreg = 0
		denypreg = 0
		daylastperiod = daystart
		dynamic $d_cycreport_update
		gs 'stat'
	elseif ARGS[0] = 4:
		thinkpreg = 0
		denypreg = 0
		clr
		pl '上次月經是幾天前?'
		pl '目前: <<pertemp>>'
		pl '<a href="exec: pertemp += 1 & clr & dynamic $d_cycreport_actor, 4">+1</a> <a href="exec: pertemp += 5 & clr & dynamic $d_cycreport_actor, 4">+5</a> <a href="exec: pertemp += 10 & clr & dynamic $d_cycreport_actor, 4">+10</a> <a href="exec: pertemp = 0 & clr & dynamic $d_cycreport_actor, 4">Reset</a> <a href="exec: daylastperiod = daystart - pertemp & dynamic $d_cycreport_update & gs ''stat''">確認</a>'
	elseif ARGS[0] = 5:
		thinkpreg = 0
		denypreg = 0
		$cycreport_txt = '妳認為妳不會再有月經了.' & !with 904
		gs 'stat'
	elseif ARGS[0] = 6:
		clr
		pl '上次月經是幾天前?'
		pl '目前: <<pertemp>>'
		pl '<a href="exec: pertemp += 1 & clr & dynamic $d_cycreport_actor, 6">+1</a> <a href="exec: pertemp += 5 & clr & dynamic $d_cycreport_actor, 6">+5</a> <a href="exec: pertemp += 10 & clr & dynamic $d_cycreport_actor, 6">+10</a> <a href="exec: pertemp = 0 & clr & dynamic $d_cycreport_actor, 6">Reset</a> <a href="exec: daylastperiod = daystart - pertemp & dynamic $d_cycreport_update & gs ''stat''">確認</a>'
	elseif ARGS[0] = 7:
		temp = arrsize('$wombpotfath')
		if temp <= 0: 
			$wombpotfath[0] = 'Unknown'
			FathpotAdd = 1
			Fathpotcheck = 0
			temp = arrsize('$cumarrnam')
			:potfakefathaddLoop
			if temp > Fathpotcheck:
				if cumarrkno[Fathpotcheck] = 1:
					$wombpotfath[Fathpotadd] = $cumarrnam[Fathpotcheck]
					Fathpotadd += 1
				end
				Fathpotcheck += 1
				Jump 'potfakefathaddloop'
			end
			temp = arrsize('$wombpotfath')
		end
		pl '<a href="exec: $wombthfath = ''<<$wombpotfath[temp]>>'' & dynamic $d_cycreport_actor, 2"><<$wombpotfath[temp]>></a>'
		:cyccustfathchoice
		if temp => 0:
			pl '<a href="exec: $wombthfath = ''<<$wombpotfath[temp]>>'' & dynamic $d_cycreport_actor, 2"><<$wombpotfath[temp]>></a>'
			temp -= 1
			jump 'cyccustfathchoice'
		end
	elseif ARGS[0] = 10:
		temp = arrsize('Babyptype')
		temp -= 1
		$cycreport_txt = '妳正在坐月子. 上次生產日期是 <<monthkid[temp]>> / <<daykid[temp]>> / <<yearkid[temp]>>.'
		gs 'stat'
	end
}

$d_cycreport_update = {
	if thinkpreg = 1 or knowpreg = 1: dynamic $d_cycreport_upduedate
	if mesec > 0:
		$cycreport_txt = '妳的月經來了.'
	else
		if cyccustom = 1:
			if preg = 1 and knowpreg = 1:
				$cycreport_txt = '妳懷孕了. 預產期是 <<$pregduedate>>.'
				if $wombthfath ! 'Unknown': $cycreport_txt = '<<$cycreport_txt>> 妳認為孩子的父親是 <<$wombthfath>>'
			elseif knowpregloss = 1:
				$cycreport_txt = '妳正在孕後恢復期.'
			elseif knowpregrecover = 1:
				$cycreport_txt = '妳正在坐月子.'
			elseif menoage <= age:
				$cycreport_txt = '妳的更年期到了(停經).'
			else
				temp[1] = daystart-daylastperiod
				if tabletkishot > 0:
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. <font color=green>保護:避孕針.</font>'
				elseif pillcon > 30000:
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. <font color=green>保護:避孕藥.</font>'
				elseif pillcon > 8000 and (FocH > 70 or ovulate > 0):
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. 保護:避孕藥, 藥有問題. 今天是<font color=red>危險日</font>'
				elseif pillcon > 8000:
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. 保護:避孕藥, 已失效.'
				elseif daystart = tabletkiday:
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. 保護:避孕藥, 尚未生效(要長期服用).'
				elseif FocH > 70 or ovulate > 0:
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前. 今天是<font color=red>危險日.</font>'
				else
					$cycreport_txt = '上次月經在 <<temp[1]>> 天前.'
				end
			end
		elseif denypreg = 1:
			if pregchem < 525:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '沒事, 妳只是月經不規則而已.'
				elseif pregchem < 1863:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '妳得了奇怪的感冒, 讓妳覺得不舒服.'
				elseif pregchem < 4000:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '奇怪的感冒還沒好. 妳決定要多運動, 減掉因為感冒帶來的脂肪.'
				elseif pregchem < 6000:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '妳的體重一直下不去...'
				elseif pregchem < 7500:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '那些人真沒禮貌, 一直問妳什麼時候生--妳沒有懷孕!'
				elseif pregchem >= 7500:
					denypreg = 1
					thinkpreg = 0
					$cycreport_txt = '如果這是懷孕, 現在應該已經要生了. 可見<B>這不是懷孕</B>.'
			else
				temp = daystart-daylastperiod
				$cycreport_txt = '妳記得上次月經是 <<temp>> 天前來的.'
			end
		elseif cyccustom = 0:
			if $cycreport_txt = 'In labour':
				$cycreport_txt = '產前陣痛中'
			elseif thinkpreg = 1 or knowpreg = 1:
				if knowpreg = 0: $temp = ' 認為妳'
				if $wombthfath = 'Unknown' or $wombthfath = '':
					$cycreport_txt = '妳<<$temp>>懷孕了. 妳不太確定孩子的父親是誰. 根據上次月經推算, 預產期是 <<$pregduedate>>.'
				else
					$cycreport_txt = '妳<<$temp>>懷上了 <<$wombthfath>> 的孩子. 根據上次月經推算, 預產期是 <<$pregduedate>>.'
				end	
			elseif $cycreport_txt ! '妳認為妳不會再有月經了.':
				temp = daystart-daylastperiod &!with 819
				$cycreport_txt = '妳記得上次月經是 <<temp>> 天前來的.'
			end
		end
	end
}

$d_cycreport_upduedate = {
	temp = (280 + daylastperiod) - daystart
	pregdueday = day
	pregduemonth = month
	pregdueyear = year
	:pregduedateloop
	if pregduemonth = 1 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 2 and temp > 28 - pregdueday:
		pregduemonth += 1
		temp -= 28 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 3 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 4 and temp > 30 - pregdueday:
		pregduemonth += 1
		temp -= 30 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 5 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 6 and temp > 30 - pregdueday:
		pregduemonth += 1
		temp -= 30 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 7 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 8 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 9 and temp > 30 - pregdueday:
		pregduemonth += 1
		temp -= 30 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 10 and temp > 31 - pregdueday:
		pregduemonth += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 11 and temp > 30 - pregdueday:
		pregduemonth += 1
		temp -= 30 - pregdueday
		pregdueday = 0
	elseif pregduemonth = 12 and temp > 31 - pregdueday:
		pregduemonth = 1
		pregdueyear += 1
		temp -= 31 - pregdueday
		pregdueday = 0
	else
		pregdueday = temp
		temp = 0
	end
	if temp > 0: jump 'pregduedateloop'
	$pregduedate = '<<pregduemonth>> / <<pregdueday>> / <<pregdueyear>>'
}

$d_pregmovement = {
	if denypreg = 0 and pregchem > 0:
		if thinkpreg > 0 and knowpreg = 0:
			knowpreg = 1
			if kid <= 0:
				'首次感到胎動讓妳心跳加速. 畢竟這是妳第一次懷孕.'
			else
				'胎動佐證了妳的想法. 妳真的又懷孕了.'
			end
		elseif knowpreg > 0:
			temprand = rand(0,2)
			if temprand = 0:
				if pregchem > 5000:
					'寶寶的胎動非常明顯, 妳的肚皮明顯跳了一下.'
				else
					'妳感覺到寶寶在子宮裡動了一下.'
				end
			elseif temprand = 1:
				if pregchem > 5000:
					'寶寶用力的踢了一下, 痛楚讓妳不禁*嘶*了一聲.'
				else
					'妳感到寶寶踢了一下.'
				end
			elseif temprand = 2:
				if pregchem > 5000:
					'妳感到寶寶踢了一下. 低頭一看, 妳的肚皮因此明顯地顫動.'
				else
					'妳感到寶寶踢了一下.'
				end
			end
		else
			if cyccustom = 1:
				if pregchem < 4000:
					if kid <= 0 :
						'妳的肚子有種奇怪的感覺, 也許妳該去看醫生?'
					else
						'妳感到像是體內有寶寶在動--妳懷孕了嗎?'
					end
				else
					knowpreg = 1
					if kid <= 0:
						'很明顯, 妳懷孕了.'
					else
						'很明顯, 妳又懷孕了.'
					end
				end
			end
		end
	elseif pregchem > 0:
		if kid > 0 and PregChem > 3903:
			'妳很想再說服自己, 但是實在沒辦法了. 膨大的肚皮和那些胎動, 妳又懷孕了.'
			denypreg = 0
			dynamic $d_cycreport_upduedate
			$cycreport_txt = '妳懷孕了.'
		else
			'妳感到肚子裡有東西在動. 也許只是脹氣.'
		end
	end
}

