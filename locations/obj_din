		else
			p '你有好幾個小孩:'
			:LoopKidCounter
			if i <= kid:
				if i > 1: p ','
				p '<<i>>. <<$kidname[i]>> 是 <<$polreb[i]>>, 生日 <<daykid[i]>>-<<monthkid[i]>>-<<yearkid[i]>>. 今年 <<kidage[i]>> 歲'
				i += 1
				jump 'LoopKidCounter'
			end
			pl '.'
		end
		nl
	end
 	if workPTU > 1:pl'教師教學名聲 : <<teachlevel>>'

	if SchoolAtestat = 0 and StoryLine > 0:
