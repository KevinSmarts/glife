	end
}

$husb_cheat_count = {
if husband > 0:
	if guy > before_h_guy : 
		guy_cheat += guy - guy_cheat
		before_h_guy = guy
	end
	if girl > before_h_girl:
		girl_cheat += girl - before_h_girl
		before_h_girl = girl
	end
end
	if before_h_girl > 0 and before_h_guy > 0:
		pl'在婚後, 妳與 <<guy_cheat>> 個男性 和 <<girl_cheat>> 個女性 有不正當關係'
	elseif before_h_guy > 0:
		pl'在婚後, 妳與 <<guy_cheat>> 個男性 有不正當關係'
	elseif before_h_girl > 0:
		pl'在婚後, 妳與 <<girl_cheat>> 個女性 有不正當關係'
	end
}

