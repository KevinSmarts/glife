$objdescrip = {
	gs'body'
	clr
	pl'<center><img src="images/pic/icon/description.png"><br></center>'
	pl '姓名 <<$name>> <<$surname>>.'
	pl '今年 <<age>> 歲, 外表像是 <<vidage>> 歲.'
	if age = vidage:
		pl '今年 <<age>> 歲.'
	else
		pl '今年 <<age>> 歲, 但看起來像是 <<vidage>> 歲.'
	end
	!!!0.2.6 extension
	if StoryLine > 0:
		pl '家人和朋友有時候會叫妳 <<$nickname>>'
	else
		pl '朋友們有時候會叫妳 <<$nickname>>'
	end
	!!!

	if birthday <= 9 and birthmonth <= 9:
		$birthdayD = '0<<birthday>>.0<<birthmonth>>.<<birthyear>>.'
	elseif birthday > 9 and birthmonth <= 9:
		$birthdayD = '<<birthday>>.0<<birthmonth>>.<<birthyear>>.'
	elseif birthday > 9 and birthmonth > 9:
		$birthdayD = '<<birthday>>.<<birthmonth>>.<<birthyear>>.'
	else
		$birthdayD = '0<<birthday>>.<<birthmonth>>.<<birthyear>>.'
	end

	pl '生日 <<$birthdayD>>'

	pl'身高 <<rost>> cm, 體重 <<ves>> kg.'
	pl '<<$body>>'
	pl'三圍是 <a href="exec:dynamic $objbody"> <<grudi>> - <<talia>> - <<bedra>></a>, <a href="exec:dynamic $din_show_tits">胸部<<$titsize>></a>, 腰圍跟臀圍差 <<bedra-talia>> cm'
	pl '<<$vnesh>>'
	if avatar_hair = 1:
		pl'頭髮: <<$av_hair>>.'
	else
		pl' <<$hair>>'
	end
	pl '<<$lip>>'
	pl '<<$throat>>'
	pl '<<$skin>>'
	pl '<<$glaza>>'
	pl '<<$mop>>'
	pl '<<$leghair>>'
	pl '<<$lobok>>'
	pl '<<$vagina>>'
	pl '<<$anus>>'
	nl

	if salo = 0 and musle > 0 and dounspell = 0:pl '<b><font color="red">妳有點營養不良, 應該按時吃點東西.</font></b>'
	if salo = 0 and musle = 0 and dounspell = 0:pl '<b><font color="red">妳嚴重地營養不良, 已經開始消耗自己的肌肉了. 妳看起來隨時會昏過去的樣子.</font></b>'
	if curly > 0:pl'燙髮還能維持 <<curly>> 天.'
	!if zub = 1:$zuba = ' tooth'
	!if zub >= 2:$zuba = 'x teeth'
	if zub > 0:pl'妳缺了 <<zub>> 顆牙.'

	!!pl 'vnesh <<vnesh>>'
	!!pl 'curlykoef <<curlykoef>> a maximum of 1'
	!!pl 'tits <<tits>> A maximum of 6'
	!!pl 'lip <<lip>> a maximum of 4'
	!!pl 'tanKoef <<tanKoef>> a maximum of 1'
	!!pl 'skin <<skin>> a maximum of 4'
	!!pl 'mop <<mop>> a maximum of 4'
	!!pl 'resnic <<resnic>> a maximum of 2'
	!!pl 'glaraz <<glaraz>> a maximum of 3'
	!!pl 'bodykoef <<bodykoef>> a maximum of 8'
	!!pl 'legkoef <<legkoef>> a maximum of 3'
	!!pl 'lobkoef <<lobkoef>> a maximum of 3'
	!!pl 'odevnesh <<odevnesh>> a maximum of 15'
	!!pl 'sweat <<sweat>> the maximum 0'
	!!pl 'hapri <<hapri>> a maximum of 1'

	!!!!!!!! Maruda Hair Mod
	if hairlength > 41 and hscrunch >= 1:
		if hscrunchw = 0 and hpingripw = 0:pl '妳可以用髮圈把頭髮束成 <a href="exec:hscrunchw = 1 & dynamic $objdescrip">馬尾</a>. 妳還有 <<hscrunch>> 條髮圈.'
		if hscrunchw = 1:pl '<a href="exec:hscrunchw = 0 & dynamic $objdescrip">拆掉</a>妳的馬尾.'
	end

	if hairlength > 41 and hpingrip >= 1:
		if hscrunchw = 0 and hpingripw = 0:pl '妳可以用髮夾把頭髮 <a href="exec:hpingripw = 1 & dynamic $objdescrip">盤高</a>. 妳還有 <<hpingrip>> 條髮夾.'
