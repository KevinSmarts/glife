$objautosave = {
	if oldobjmenu = 0:
		if disable_autosave = 1:
			disable_autosave = 0
		else
			disable_autosave = 1
		end
	elseif oldobjmenu = 1:
		if disable_autosave = 1:
			disable_autosave = 0
			delobj '自動存檔:OFF'
			addobj '自動存檔:ON'
		else
			disable_autosave = 1
			delobj '自動存檔:ON'
			addobj '自動存檔:OFF'
		end
	end
	gs'stat'
}

