$cheatmenu['force_preg'] = {
	if mesec ! 0: mesec = 0
	if FocH ! 0: FocH = 0
	if EggRH ! 0: EggRH = 0
	if Ovulate ! 0: Ovulate = 0
	if UnfertEgg ! 0: UnfertEgg = 0
	if LutH ! 0: LutH = 0
	if RecovH ! 0: RecovH = 0
	preg = 1
	cycle = 5
	PregChem = 150
	$wombthfath = '精子捐贈者'
	babyembryo += 1
	if fertegg = 0:
		Nextbaby = arrsize('$ChildFath')
		Temppolkid = rand(0,1)
		polkid[nextbaby] = Temppolkid
		$kidname[nextbaby] = 'unborn'
		kidage[nextbaby] = 0
		daykid[nextbaby] = 0
		monthkid[nextbaby] = 0
		yearkid[nextbaby] = 0
		Babyptype[nextbaby] = 0
		$ChildFath[nextbaby] = 'A sperm donor'
		$ChildThFath[nextbaby] = 'A sperm donor'
		hairkid[nextbaby] = rand(0, 3)
		eyeskid[nextbaby] = rand(0, 3)
		ChildConType[nextbaby] = 0
		Babyptype[nextbaby] = 1
	else
		fertegg -= 1
	end
}

$cheatmenu['remove_preg'] = {
	Nextbaby = arrsize('$ChildFath') - 1
	killvar 'polkid',nextbaby
	killvar '$kidname',nextbaby
	killvar 'kidage',nextbaby
	killvar 'daykid',nextbaby
	killvar 'monthkid',nextbaby
	killvar 'yearkid',nextbaby
	killvar 'Babyptype',nextbaby
	killvar '$ChildFath',nextbaby
	killvar '$ChildThFath',nextbaby
	killvar 'hairkid',nextbaby
	killvar 'eyeskid',nextbaby
	killvar 'ChildConType',nextbaby
	killvar 'Babyptype',nextbaby
	if babyembryo > 0: babyembryo -= 1
	if preg > 0 and babyembryo < 1:
	mesec = 48
	FocH = 0
	EggRH = 0
	Ovulate = 0
	fertegg = 0
	UnfertEgg = 0
	PregChem = 0
	preg = 0
	LutH = 0
	RecovH = 0
	babyembryo = 0
	cycle = 0
	end
}
$cheatmenu['cycle'] = {
	if cycle ! 5:
		EggRH = 0
		LutH = 0
		Ovulate = 0
		UnfertEgg = 0
		RecovH = 0
		mesec = 0
		'<a href="exec:Cycle = 0 & mesec = 96">月經期</a>'
		'<a href="exec:Cycle = 1">卵泡期</a>'
		'<a href="exec:Cycle = 2 & Ovulate = 36 & EggRH = 160">排卵期</a>'
		'<a href="exec:Cycle = 3">黃體期</a>'
	end
}
$cheatmenu['display'] = {
	gs'stat'
	clr
	pl'<center> <a href="exec:dynamic $cheatmenu"><b>界面設定</b></a></center>'
	nl
	if Enable_faceturn = 1:
		pl' <a href="exec:Enable_faceturn = 0 & dynamic $cheatmenu[''display'']">隨時顯示頭像: ON</a>'
	else
		pl' <a href="exec:Enable_faceturn = 1 & dynamic $cheatmenu[''display'']">隨時顯示頭像: OFF</a>'
	end
	nl
	if ETOmenu = 1:
		pl' <a href="exec:ETOmenu = 0 & dynamic $cheatmenu[''display'']">顯示模式: 文字</a>'
	else
		pl' <a href="exec:ETOmenu = 1 & dynamic $cheatmenu[''display'']">顯示模式: 長條</a>'
	end
	if Enable_showattr = 1:
		pl' <a href="exec:Enable_showattr = 0 & dynamic $cheatmenu[''display'']">顯示屬性: ON</a>'
	else
		pl' <a href="exec:Enable_showattr = 1 & dynamic $cheatmenu[''display'']">顯示屬性: OFF</a>'
	end
	if Enable_showskill = 1:
		pl' <a href="exec:Enable_showskill = 0 & dynamic $cheatmenu[''display'']">顯示技能: ON</a>'
	else
		pl' <a href="exec:Enable_showskill = 1 & dynamic $cheatmenu[''display'']">顯示技能: OFF</a>'
	end
	if Enable_showrelation = 1:
		pl' <a href="exec:Enable_showrelation = 0 & dynamic $cheatmenu[''display'']">顯示好感度: ON</a>'
	else
		pl' <a href="exec:Enable_showrelation = 1 & dynamic $cheatmenu[''display'']">顯示好感度: OFF</a>'
	end
	nl
	if Enable_clothdetail = 1:
		pl'<a href="exec:Enable_clothdetail = 0 & dynamic $cheatmenu[''display'']">衣櫃清單顯示細節: ON</a>'
	else
		pl'<a href="exec:Enable_clothdetail = 1 & dynamic $cheatmenu[''display'']">衣櫃清單顯示細節: OFF</a>'
	end	
	if Enable_clothwidth<0:Enable_clothwidth=0
	if Enable_clothwidth > 0:
		pl'指定衣櫃縮圖寬度: <a href="exec:Enable_clothwidth=input(''輸入衣櫃縮圖寬度'') & dynamic $cheatmenu[''display'']"><<Enable_clothwidth>></a>, <a href="exec:Enable_clothwidth=0 & dynamic $cheatmenu[''display'']">ON</a>'
	else
		pl'指定衣櫃縮圖寬度: <a href="exec:Enable_clothwidth=input(''輸入衣櫃縮圖寬度'') & dynamic $cheatmenu[''display'']">OFF</a>'
	end
	if Enable_tablemap = 1:
		pl' <a href="exec:Enable_tablemap = 0 & dynamic $cheatmenu[''display'']">以表格顯示朋友: ON</a>'
	else
		pl' <a href="exec:Enable_tablemap = 1 & dynamic $cheatmenu[''display'']">以表格顯示朋友: OFF</a>'
	end
	if Enable_towntable = 1:
		pl' <a href="exec:Enable_towntable = 0 & dynamic $cheatmenu[''display'']">並列小鎮圖片與簡介: ON</a>'
	else
		pl' <a href="exec:Enable_towntable = 1 & dynamic $cheatmenu[''display'']">並列小鎮圖片與簡介: OFF</a>'
	end
	nl
	if Enable_statfsize<0:Enable_statfsize=0
	if Enable_statfsize > 0:
		pl'指定狀態欄字體大小: <a href="exec:Enable_statfsize=input(''輸入狀態欄字體大小'') & dynamic $cheatmenu[''display'']"><<Enable_statfsize>></a>, <a href="exec:Enable_statfsize=0 & dynamic $cheatmenu[''display'']">ON</a>'
	else
		pl'指定狀態欄字體大小: <a href="exec:Enable_statfsize=input(''輸入狀態欄字體大小'') & dynamic $cheatmenu[''display'']">OFF</a>'
	end
	if cheatStatusBars = 1:
		pl' <a href="exec:cheatStatusBars = 0 & dynamic $cheatmenu[''display'']">較大的狀態欄圖表</a>'
	else
		pl' <a href="exec:cheatStatusBars = 1 & dynamic $cheatmenu[''display'']">較小的狀態欄圖表</a>'
	end
	if Enable_Android = 1:
		pl 'Android模式下, 用功能列表的<font color=blue>雜項-重新整理</font>以更新狀態視窗.'
		pl'<a href="exec:Enable_Android = 0 & dynamic $cheatmenu[''display'']">Android 模式:ON</a>'
	else
		pl'<a href="exec:Enable_Android = 1 & dynamic $cheatmenu[''display'']">Android 模式:OFF</a>'
	end	
}

