# Cheatmenu_din
!translation to Chinese, by illume
!All the active dynamics for the cheat menu are here
$cheatmenu['work'] = {
	if cheatWork = 0:
		cheatWork = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatWork = 1:
		cheatWork = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['hair'] = {
	if cheatHapri = 0:
		cheatHapri = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatHapri = 1:
		cheatHapri = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['enema'] = {
	if cheatKlisma = 0:
		cheatKlisma = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatKlisma = 1:
		cheatKlisma = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['nomakeup'] = {
	if cheatNomakeup = 0:
		cheatNomakeup = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatNomakeup = 1:
		cheatNomakeup = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['nosweat'] = {
	if cheatNosweat = 0:
		cheatNosweat = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatNosweat = 1:
		cheatNosweat = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['noEat'] = {
	if cheatNoEat = 0:
		cheatNoEat = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatNoEat = 1:
		cheatNoEat = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['noSleep'] = {
	if cheatNoSleep = 0:
		cheatNoSleep = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatNoSleep = 1:
		cheatNoSleep = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['health'] = {
	if cheatHealth = 0:
		cheatHealth = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatHealth = 1:
		cheatHealth = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['Pirate'] = {
	if cheatPirate = 0:
		cheatPirate = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatPirate = 1:
		cheatPirate = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['age'] = {
	age = input("How many years you are now?")
	if age <= 15:age = 15
	dynamic $cheatmenu['looks']
}

$cheatmenu['visualage'] = {
	vidage = input("For how many years you want to look?")
	if vidage <= 14:vidage = 14
	dynamic $cheatmenu['looks']
}

$cheatmenu['money'] = {
	money = input("How much money do you want to have?")
	if money < 0:money = 0
	dynamic $cheatmenu
}

$cheatmenu['bodyMod'] = {
   dounspell = 1
   TaliaDoun = input ("Waist in cm?")
   if TaliaDoun <= 0:TaliaDoun = 20
   bedraDoun = input ("Hips in cm?")
   if bedraDoun <= 0:bedraDoun = 40
}

$cheatmenu['addict'] = {
	if NarkImmune = 0:
		NarkImmune = 1
		dynamic $cheatmenu['permanent']
		exit
	elseif NarkImmune = 1:
		NarkImmune = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['slutshot'] = {
	if cheatSlut = 0:
		cheatSlut = 1
		cycle = 6
		dynamic $cheatmenu['permanent']
		exit
	elseif cheatSlut = 1:
		cheatSlut = 0
		cycle = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['Vibrator'] = {
	if cheatVib = 0:
		cheatVib = 1
		dynamic $cheatmenu['permanent']
	elseif cheatVib = 1:
		cheatVib = 0
		dynamic $cheatmenu['permanent']
	end
}

$cheatmenu['permanent'] = {
	gs'stat'
	clr
	if cheatWork = 0:$cheatWork = 'OFF'
	if cheatWork = 1:$cheatWork = '<b>ON</b>'
	if cheatHapri = 0:$cheatHapri = 'OFF'
	if cheatHapri = 1:$cheatHapri = '<b>ON</b>'
	if cheatKlisma = 0:$cheatKlisma = 'OFF'
	if cheatKlisma = 1:$cheatKlisma = '<b>ON</b>'
	if cheatNomakeup = 0:$cheatNomakeup = 'OFF'
	if cheatNomakeup = 1:$cheatNomakeup = '<b>ON</b>'
	if cheatNosweat = 0:$cheatNosweat = 'OFF'
	if cheatNosweat = 1:$cheatNosweat = '<b>ON</b>'
	if cheatNoEat = 0:$cheatNoEat = 'OFF'
	if cheatNoEat = 1:$cheatNoEat = '<b>ON</b>'
	if cheatNoSleep = 0:$cheatNoSleep = 'OFF'
	if cheatNoSleep = 1:$cheatNoSleep = '<b>ON</b>'
	if cheatHealth = 0:$cheatHealth = 'OFF'
	if cheatHealth = 1:$cheatHealth = '<b>ON</b>'
	if NarkImmune = 0:$NarkImmune = 'OFF'
	if NarkImmune = 1:$NarkImmune = '<b>ON</b>'
	if cheatSlut = 0:$cheatSlut = 'NO'
	if cheatSlut = 1:$cheatSlut = '<b>YES</b>'
	if cheatVib = 0:$cheatVib = 'NO'
	if cheatVib = 1:$cheatVib = '<b>YES</b>'
	if cheatpirate = 0:$cheatpirate = 'NO'
	if cheatpirate = 1:$cheatpirate = '<b>YES</b>'

	pl'<center><a href="exec:dynamic $cheatmenu"><b>週期性功能設定</b></a></center>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''work'']">忽略工作條件需求</a>: <<$cheatWork>>'
	pl'<a href="exec:dynamic $cheatmenu[''hair'']">自動梳頭髮</a>: <<$cheatHapri>>'
	pl'<a href="exec:dynamic $cheatmenu[''enema'']">自動灌腸</a>: <<$cheatKlisma>>'
	pl'<a href="exec:dynamic $cheatmenu[''nomakeup'']">化妝不花掉</a>: <<$cheatNomakeup>>'
	pl'<a href="exec:dynamic $cheatmenu[''nosweat'']">不流汗</a>: <<$cheatNosweat>>'
	pl'<a href="exec:dynamic $cheatmenu[''noEat'']">不用吃喝</a>: <<$cheatNoEat>>'
	pl'<a href="exec:dynamic $cheatmenu[''noSleep'']">不用睡覺</a>: <<$cheatNoSleep>>'
	pl'<a href="exec:dynamic $cheatmenu[''health'']">不會得性病</a>: <<$cheatHealth>>'
	pl'<a href="exec:dynamic $cheatmenu[''addict'']">不會上癮</a>: <<$NarkImmune>>'
	pl'<a href="exec:dynamic $cheatmenu[''Vibrator'']">允許裝備跳蛋睡覺</a>: <<$cheatVib>>'
	pl'<a href="exec:dynamic $cheatmenu[''Pirate'']">狗 Pirate 總是性奮</a>: <<$cheatpirate>>'
	if Enable_sleep_wait = 1:
		pl'<a href="exec:Enable_sleep_wait = 0 & dynamic $cheatmenu[''permanent'']">睡覺不停頓:ON</a>'
	else
		pl'<a href="exec:Enable_sleep_wait = 1 & dynamic $cheatmenu[''permanent'']">睡覺不停頓:OFF</a>'
	end
    pl''
	pl'酒醉: <a href="exec:alko = 0 & dynamic $cheatmenu[''permanent'']">清醒</a>, <a href="exec:alko = 2 & dynamic $cheatmenu[''permanent'']">微醺</a>, <a href="exec:alko = 4 & dynamic $cheatmenu[''permanent'']">醉酒</a>, <a href="exec:alko = 7 & dynamic $cheatmenu[''permanent'']">狂飲</a>, <a href="exec:alko = 11 & dynamic $cheatmenu[''permanent'']">爛醉</a>'
	pl''
	pl'性奮: <a href="exec:horny = 100 & dynamic $cheatmenu[''permanent'']">性奮</a>, <a href="exec:horny = 0 & dynamic $cheatmenu[''permanent'']">平靜</a>'
	pl'<a href="exec:dynamic $cheatmenu[''bodyMod''] & dynamic $cheatmenu[''permanent'']">修改身體係數</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''slutshot'']">妓女模式:興奮50+, 無懷孕, 生理期</a>: <<$cheatSlut>>'
	pl''
	if Enable_autotampon = 1:
		pl' <a href="exec:Enable_autotampon = 0 & dynamic $cheatmenu[''permanent'']">自動衛生棉: ON</a>'
	else
		pl' <a href="exec:Enable_autotampon = 1 & dynamic $cheatmenu[''permanent'']">自動衛生棉: OFF</a>'
	end
	pl''
	if Enable_nodream > 0:
		pl'不做夢的機率: <a href="exec:Enable_nodream=input(''輸入不做夢的機率 (%)'') & dynamic $cheatmenu[''permanent'']"><<din_cheat_nodream>>%</a>, <a href="exec:din_cheat_nodream=0 & dynamic $cheatmenu[''permanent'']">ON</a>'
	else
		pl'不做夢的機率: <a href="exec:Enable_nodream=input(''輸入不做夢的機率 (%)'') & dynamic $cheatmenu[''permanent'']">OFF</a>'
	end & !1.2.3 new function
	if Enable_skipnoevclass > 0:
		pl'只允許有後續的上課事件(Anatoly E.), 機率: <a href="exec:Enable_skipnoevclass=input(''輸入上課事件發生的機率 (%)'') & dynamic $cheatmenu[''permanent'']"><<Enable_skipnoevclass>>%</a>, <a href="exec:Enable_skipnoevclass=0 & dynamic $cheatmenu[''permanent'']">ON</a>'
	else
		pl'只允許有後續的上課事件(Anatoly E.): <a href="exec:Enable_skipnoevclass=input(''輸入上課事件發生的機率 (%)'') & dynamic $cheatmenu[''permanent'']">OFF</a>'
	end
	nl
	pl'<B>警告</B>:部份劇情需要身上有精液/汗水/非處女, 以下功能可能導致劇情bug/事件無法觸發, 小心使用'
	if Enable_clearcum = 1:
		pl' <a href="exec:Enable_clearcum = 0 & dynamic $cheatmenu[''permanent'']">自動清除精液: ON</a>'
	else
		pl' <a href="exec:Enable_clearcum = 1 & dynamic $cheatmenu[''permanent'']">自動清除精液: OFF</a>'
	end
	!{
	if Enable_fbreath = 1:
		pl' <a href="exec:Enable_fbreath = 0 & dynamic $cheatmenu[''permanent'']">總是口氣清新: ON</a>'
	else
		pl' <a href="exec:Enable_fbreath = 1 & dynamic $cheatmenu[''permanent'']">總是口氣清新: OFF</a>'
	end
	!remove them, add later after OOx system done
	}
	if Enable_clearwrite = 1:
		pl' <a href="exec:Enable_clearwrite = 0 & dynamic $cheatmenu[''permanent'']">自動清除塗鴉: ON</a>'
	else
		pl' <a href="exec:Enable_clearwrite = 1 & dynamic $cheatmenu[''permanent'']">自動清除塗鴉: OFF</a>'
	end
	if Enable_lockhorny > 0:
		pl'興奮度鎖定: <a href="exec:Enable_lockhorny=1+input(''輸入要鎖定的興奮度'') & dynamic $cheatmenu[''permanent'']"><<Enable_lockhorny-1>></a>, <a href="exec:Enable_lockhorny=0 & dynamic $cheatmenu[''permanent'']">ON</a>'
	else
		pl'興奮度鎖定: <a href="exec:Enable_lockhorny=1+input(''輸入要鎖定的興奮度'') & dynamic $cheatmenu[''permanent'']">OFF</a>'
	end
	if Enable_Hymenrestore = 1:
		pl' <a href="exec:Enable_Hymenrestore = 0 & dynamic $cheatmenu[''permanent'']">自動再生處女膜: ON</a>'
	else
		pl' <a href="exec:Enable_Hymenrestore = 1 & dynamic $cheatmenu[''permanent'']">自動再生處女膜: OFF</a>'
	end
	if Enable_tightrestore = 1:
		pl' <a href="exec:Enable_tightrestore = 0 & dynamic $cheatmenu[''permanent'']">自動私處緊縮: ON</a>'
	else
		pl' <a href="exec:Enable_tightrestore = 1 & dynamic $cheatmenu[''permanent'']">自動私處緊縮: OFF</a>'
	end
}
 
$cheatmenu['avatar_hair_set'] = {
	$av_hair = input("輸入妳自訂的頭髮敘述:")
	if $av_hair='':exit
	avatar_hair = 1
	dynamic $cheatmenu['looks']
}

$cheatmenu['rename'] = {
	$name = input("輸入名字 (空白為預設值 Svetlana)")

	if $name = '':$name = 'Svetlana'

	$surname = input("輸入姓氏 (空白為預設值 Lebedev)")

	if $surname = '':$surname = 'Lebedev'

	!!!0.2.6 addition
	$nickname = input("輸入暱稱 (空白為預設值 Sveta)")

	if $nickname = '':$nickname = 'Sveta'
}

$cheatmenu['looks'] = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $cheatmenu"><b>外貌設定 1</b></a></center>'
	pl''
	pl'<<$name>> <<$surname>> (<<$nickname>>). <a href="exec:dynamic $cheatmenu[''rename'']">改名</a>'
	pl'固定使用放在與girllife.esp同資料夾的avatar.jpg. 建議大小為400*400'
	if player_avatar = 1:
		pl' <a href="exec:player_avatar = 0 & dynamic $cheatmenu[''looks'']">自訂頭像: ON</a>'
	else
		pl' <a href="exec:player_avatar = 1 & dynamic $cheatmenu[''looks'']">自訂頭像: OFF</a>'
	end	
	if avatar_hair = 1:
		pl'<a href="exec:avatar_hair = 0 & dynamic $cheatmenu[''looks'']">自訂頭髮敘述: ON</a>'
		pl'<<$av_hair>>'
	else
		pl'<a href="exec:dynamic $cheatmenu[''avatar_hair_set'']">自訂頭髮敘述: OFF</a>'
	end
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''parameters'']">顯示身體設計參數</a>'
	pl''
	pl'你今年 <<age>> 歲'
	if age >= 0:pl'<a href="exec:dynamic $cheatmenu[''age'']">改變實際年齡</a>'
	pl'<a href=>看起來像是 <<vidage>> 歲</a>'
	if vidage >= 0:pl'<a href="exec:dynamic $cheatmenu[''visualage'']">改變外貌年齡</a>'
	pl''
	pl '妳的胸部是 <<$titsize>>'
	if tits => 0 and tits < 10:pl'<a href="exec:silicone += 5 & dynamic $cheatmenu[''looks'']">胸部增大(矽膠)</a>'
	if silicone => 1:pl'<a href="exec:silicone -= 5 & dynamic $cheatmenu[''looks'']">胸部縮小(矽膠)</a>'
	if tits => 0 and tits < 10:pl'<a href="exec:nbsize += 5 & dynamic $cheatmenu[''looks'']">胸部增大(自然)</a>'
	if nbsize => 1:pl'<a href="exec:nbsize -= 5 & dynamic $cheatmenu[''looks'']">胸部縮小(自然)</a>'
	pl''
	pl'<<$lip>>'
	if lip < 4:pl'<a href="exec:lip += 1 & dynamic $cheatmenu[''looks'']">嘴唇增厚</a>'
	if lip > 0:pl'<a href="exec:lip -= 1 & dynamic $cheatmenu[''looks'']">嘴唇削薄</a>'
	pl''
	pl'<<$skin>>'
	if skin < 4:pl'<a href="exec:skin += 1 & dynamic $cheatmenu[''looks'']">改善膚質</a>'
	if skin > 0:pl'<a href="exec:skin -= 1 & dynamic $cheatmenu[''looks'']">差化膚質</a>'
	if tan >= 0:pl'<a href="exec:tan += 10 & dynamic $cheatmenu[''looks'']">增加日晒度</a>'
	if tan > 0:pl'<a href="exec:tan = 0 & dynamic $cheatmenu[''looks'']">日晒度歸零</a>'
	pl''
	pl'<<$throat>>'
	if throat <= 31:pl'<a href="exec:throat += 5 & dynamic $cheatmenu[''looks'']">增大喉嚨容量</a>'
	if throat >= 5:pl'<a href="exec:throat -= 5 & dynamic $cheatmenu[''looks'']">縮小喉嚨容量</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''looks2'']">到第二頁</a>                <a href="exec:dynamic $cheatmenu[''looks3'']">到第三頁</a>'
}
	
$cheatmenu['looks2'] = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $cheatmenu"><b>外貌設定2</b></a></center>'
	pl''
	pl'<<$vagina>>'
	if vagina > 0:pl'<a href="exec:vagina = 0 & dynamic $cheatmenu[''looks2'']">陰道復原</a>'
	if vagina <= 31:pl'<a href="exec:vagina += 5 & dynamic $cheatmenu[''looks2'']">陰道擴張</a>'
	if vagina >= 5:pl'<a href="exec:vagina -= 5 & dynamic $cheatmenu[''looks2'']">陰道緊縮</a>'
	pl '目前: 陰道每 <<vshrinkdays>> 天會自然回復 <<vshrink>>.'
	if vshrink < 10:pl'<a href="exec:vshrink += 1 & dynamic $cheatmenu[''looks2'']">增加緊縮量</a>'
	if vshrink > 0:pl'<a href="exec:vshrink -= 1 & dynamic $cheatmenu[''looks2'']">減少緊縮量</a>'
	if vshrinkdays < 10:pl'<a href="exec:vshrinkdays += 1 & dynamic $cheatmenu[''looks2'']">增加週期天數</a>'
	if vshrinkdays > 1:pl'<a href="exec:vshrinkdays -= 1 & dynamic $cheatmenu[''looks2'']">減少週期天數</a>'
	pl''
	pl'<<$anus>>'
	if anus <= 31:pl'<a href="exec:anus += 5 & dynamic $cheatmenu[''looks2'']">肛門擴張</a>'
	if anus >= 5:pl'<a href="exec:anus -= 5 & dynamic $cheatmenu[''looks2'']">肛門緊縮</a>'
	pl '目前: 肛門每 <<ashrinkdays>> 天會自然回復 <<ashrink>> '
	if ashrink < 10:pl'<a href="exec:ashrink += 1 & dynamic $cheatmenu[''looks2'']">增加緊縮量</a>'
	if ashrink > 0:pl'<a href="exec:ashrink -= 1 & dynamic $cheatmenu[''looks2'']">減少緊縮量</a>'
	if ashrinkdays < 10:pl'<a href="exec:ashrinkdays += 1 & dynamic $cheatmenu[''looks2'']">增加週期天數</a>'
	if ashrinkdays > 1:pl'<a href="exec:ashrinkdays -= 1 & dynamic $cheatmenu[''looks2'']">減少週期天數</a>'
	pl''
	if salo ! fat:pl'<a href="exec:fat = salo & dynamic $cheatmenu[''looks2'']">體脂平衡</a>'
	if salo > 0:pl'<a href="exec:salo -= 5 & fat = salo & dynamic $cheatmenu[''looks2'']">減少脂肪</a>'
	if salo > 0:pl'<a href="exec:salo = 0 & fat = salo & dynamic $cheatmenu[''looks2'']">零脂肪</a>'
	pl'脂肪 (<<salo>>): <a href="exec:salo += 5 & fat = salo & dynamic $cheatmenu[''looks2'']">+5</a> <a href="exec:salo += 10 & fat = salo & dynamic $cheatmenu[''looks'']">+10</a> <a href="exec:salo += 25 & fat = salo & dynamic $cheatmenu[''looks'']">+25</a> <a href="exec:salo += 50 & fat = salo & dynamic $cheatmenu[''looks'']">+50</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''tatoo'']">移除刺青</a>'
	pl''
	pl'<a href="exec:dynamic $cheatmenu[''looks'']">到第一頁</a>                <a href="exec:dynamic $cheatmenu[''looks3'']">到第三頁</a>'
}

$cheatmenu['looks3'] = {
	gs'stat'
	clr
	pl'<center><a href="exec:dynamic $cheatmenu"><b>回主選單</b></a></center>'
	pl''
	pl'<<$hair>>'
	if hcol =< 0:
		pl'妳現在的髮色是          <<$hcol>> <a href="exec:hcol +=1 & dynamic $cheatmenu[''looks3'']">(下一種)</a>'
	elseif hcol > 0 and hcol < 3:
		pl'妳現在的髮色是 <a href="exec:hcol -=1 & dynamic $cheatmenu[''looks3'']">(上一種)</a> <<$hcol>> <a href="exec:hcol +=1 & dynamic $cheatmenu[''looks3'']">(下一種)</a>'
	elseif hcol => 3:
		pl'妳現在的髮色是 <a href="exec:hcol -=1 & dynamic $cheatmenu[''looks3'']">(上一種)</a> <<$hcol>> (下一種)'
	end
	if hairlength > 575:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a>'
	elseif hairlength > 500 and hairlength =< 575:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a>'
	elseif hairlength =< 500 and hairlength => 100:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength -= 100 & dynamic $cheatmenu[''looks3'']">-100</a> <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 100 and hairlength => 25:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength -= 25 & dynamic $cheatmenu[''looks3'']">-25</a> <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 100 and hairlength => 25:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength -= 5 & dynamic $cheatmenu[''looks3'']">-5</a> <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	elseif hairlength < 25:
		pl'頭髮長度 (<<hairlength>>)  <a href="exec:hairlength += 5 & dynamic $cheatmenu[''looks3'']">+5</a> <a href="exec:hairlength += 25 & dynamic $cheatmenu[''looks3'']">+25</a> <a href="exec:hairlength += 100 & dynamic $cheatmenu[''looks3'']">+100</a>'
	end
	pl''
	pl'<<$glaza>>'
	if resnic < 2:pl'<a href="exec:resnic += 1 & dynamic $cheatmenu[''looks3'']">增長睫毛</a>'
	if resnic > 0:pl'<a href="exec:resnic -= 1 & dynamic $cheatmenu[''looks3'']">縮短睫毛</a>'
	if glaraz < 3:pl'<a href="exec:glaraz += 1 & dynamic $cheatmenu[''looks3'']">增大眼睛大小</a>'
	if glaraz > 0:pl'<a href="exec:glaraz -= 1 & dynamic $cheatmenu[''looks3'']">減少眼睛大小</a>'
	if glacol <= 0:
		pl'妳的眼睛現在是          <<$glacol>> <a href="exec:glacol +=1 & dynamic $cheatmenu[''looks3'']">(下一種)</a>'
	elseif glacol > 0 and glacol < 3:
		pl'妳的眼睛現在是 <a href="exec:glacol -=1 & dynamic $cheatmenu[''looks3'']">(上一種)</a> <<$glacol>> <a href="exec:glacol +=1 & dynamic $cheatmenu[''looks3'']">(下一種)</a>'
	elseif glacol >= 3:
		pl'妳的眼睛現在是 <a href="exec:glacol -=1 & dynamic $cheatmenu[''looks3'']">(上一種)</a> <<$glacol>> (下一種)'
	end
	pl''
	pl'魅力修正值:<a href="exec:Aphrodite = input({輸入魅力修正值}) & dynamic $cheatmenu[''looks3'']">【<<Aphrodite>>】</a>'
	pl'<a href="exec:dynamic $cheatmenu[''looks'']">到第一頁</a>                <a href="exec:dynamic $cheatmenu[''looks2'']">到第二頁</a>'
}

