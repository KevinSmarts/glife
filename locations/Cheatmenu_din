$cheatmenu['input_time'] = {
	inputtmp = input("請輸入4位時間(1:2請輸入0102)")
	if inputtmp/100 >= 0 and inputtmp/100 <= 23:hour = inputtmp/100
	inputtmp = inputtmp mod 100
	if inputtmp >= 0 and inputtmp <= 59:minut = inputtmp
	killvar 'inputtmp'
	dynamic $cheatmenu['time']
}

$cheatmenu['time'] = {
	gs'stat'
	clr
	pl'<center> <a href="exec:dynamic $objcheat_menu"><b>時間設定</b></a></center>'
	pl''
	pl'現在是 <<year>> 年 <<$month>> <<day>> 日, <<$week>> '
	if hour < 10:
		if minut > 9:p'<b>0<<hour>> : <<minut>></b>'
		if minut < 10:p'<b>0<<hour>> : 0<<minut>></b>'
	end
	if hour > 9:
		if minut > 9:p'<b><<hour>> : <<minut>></b>'
		if minut < 10:p'<b><<hour>> : 0<<minut>></b>'
	end
	nl
	pl'以下不會觸發時間事件'
	pl'<a href="exec:dynamic $cheatmenu[''week'']">選擇星期</a>'
	pl'<a href="exec:day += 1 & dynamic $cheatmenu[''time'']">+1 日</a>'
	pl'<a href="exec:day -= 1 & dynamic $cheatmenu[''time'']">-1 日</a>'
	pl'<a href="exec:dynamic $cheatmenu[''month'']">選擇月份</a>'
	pl'<a href="exec:year += 1 & dynamic $cheatmenu[''time'']">+1 年</a>'
	pl'<a href="exec:year -= 1 & dynamic $cheatmenu[''time'']">-1 年</a>'
	
	pl'<a href="exec:dynamic $cheatmenu[''input_time'']">輸入四位時間</a>'
	pl'以下指令 增加時間時會觸發時間事件, 請小心使用'
	$SS=''
	pl'時:'
	if hour>0:$SS='<a href="exec:hour -= 1 & dynamic $cheatmenu[''time'']">-1</a> ' else $SS='    '
	if hour!0:$SS=$SS+'<a href="exec:hour = 0 & dynamic $cheatmenu[''time'']">0</a> ' else $SS=$SS+'   '
	if hour<23:$SS=$SS+'<a href="exec:hour += 1 & dynamic $cheatmenu[''time'']">+1</a>'
	pl $SS
	$SS=''
	pl'分:'
	if minut>14:$SS+='<a href="exec:minut -= 15 & dynamic $cheatmenu[''time'']">-15</a> ' else $SS='    '
	if minut>0:$SS+='<a href="exec:minut -= 1 & dynamic $cheatmenu[''time'']">-1</a> ' else $SS='    '
	if minut!0:$SS+='<a href="exec:minut = 0 & dynamic $cheatmenu[''time'']">0</a> ' else $SS=$SS+'   '
	if minut<59:$SS+='<a href="exec:minut += 1 & dynamic $cheatmenu[''time'']">+1</a> '
	if minut<59:$SS+='<a href="exec:minut += 15 & dynamic $cheatmenu[''time'']">+15</a>'
	pl $SS
}

